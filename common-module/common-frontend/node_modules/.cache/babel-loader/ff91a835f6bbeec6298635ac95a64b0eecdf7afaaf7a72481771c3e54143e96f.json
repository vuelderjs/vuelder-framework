otification(){\n        return this.$store.state.cartModule.cart.length\n      },\n      styleElementNavbar(){\n        if(this.profileUsername){\n          return{\n            width: '270px'\n          }\n        }else{\n          return {\n            width: '400px'\n          }\n        }\n      },\n      magic(){\n        if(this.$vuetify.theme.dark) return 'Lumos'\n        else return 'Nox'\n      },\n      profileUsername(){\n        if(this.$store.state.userModule.user.username && this.$store.state.userModule.user.username.length>0) return this.$store.state.userModule.user.username.toUpperCase()\n        else return false\n      },\n      role(){\n        return this.$store.state.userModule.user.role\n      }\n    }\n  }\n  </script>\n  \n  <style scoped>\n  </style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}